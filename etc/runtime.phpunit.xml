<!-- PHPUnit Configuration File for PHPT Tests
 
     See: Makefile test-phpt file
     See: etc/bootstrap.dev.ini
     See: test/runtime folder for test files
 
-->
<phpunit backupGlobals="false"
         backupStaticAttributes="false"
         bootstrap="../bootstrap.php"
         cacheTokens="true"
         colors="false"
         convertErrorsToExceptions="true"
         convertNoticesToExceptions="true"
         convertWarningsToExceptions="true"
         processIsolation="false"
         stopOnFailure="false"
         syntaxCheck="false"
         strictMode="true"
         verbose="false">
</phpunit>